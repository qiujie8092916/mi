						position: relative;
				    float: left;
				    width: 70px;
				    height: 82px;
				    padding: 0 3px;
							
						a{
							display: block;
					    padding-top: 18px;
					    text-overflow: ellipsis;
					    white-space: nowrap;
					    overflow: hidden;
					    _zoom: 1;
					    @include animate(transition, color, .2s);
							color: rgba(255,255,255,0.7);
							text-align: center;	
							font-size: 12px;
							height: 64px;
							
							i{
								display: block;
						    height: 24px;
						    margin-bottom: 4px;
						    font-size: 24px;
						    line-height: 24px;	
							}
							
							&:hover{
						  	color: #fff;
							}
						}
						
					}
					
					.right:before{
						top: 6px;
				    right: 0; 
				    width: 1px;
				    height: 70px;	    
				    position: absolute;
						content: "";
						background-color: #665e57;
						display: table;	
					}
					
					.bottom:after{    
						bottom: 0;
						left: 3px;
				    width: 70px;
				    height: 1px;    
				    position: absolute;
						content: "";
						background-color: #665e57;
						display: table;	
					}
				}
			}
			
			.img{
				margin-right: 14px;
				@include animate(transition, box-shadow, .2s, linear);
				
				&:hover{
					box-shadow: 0 15px 30px rgba(0,0,0,0.1);
				}
			}	
		}
	}
}

