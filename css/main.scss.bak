    				border-top-color: #e53935;
    			}
    			
    			&.li-5, &.li-10{
    				border-top-color: #00c0a5;
    			}
    			
    			.thumb{
				    width: 160px;
				    margin: 0 auto 22px;
				    display: block;
    			}
    			
    			.name{
    				color: #212121;
				    margin: 0 20px 3px;
				    font-size: 14px;
				    font-weight: 400;
				    text-overflow: ellipsis;
				    white-space: nowrap;
				    overflow: hidden;	
    			}
    			
    			.desc{
    				height: 18px;
				    margin: 0 20px 12px;
				    font-size: 12px;
				    text-overflow: ellipsis;
				    white-space: nowrap;
				    overflow: hidden;
				    _zoom: 1;
				    color: #b0b0b0;	
    			}

    			.price{
    				color: #ff6700;	
    			}
				}	
			}	
		}
	}
}

