.background.fade{
	width:100%;    
	position: absolute;
  top: 0;
	left: 0;
	background-color: #000;
	z-index: 50;
	@include opacity(0);
	@include animate(transition, opacity, .4s, linear);
	
	&.in{
		@include opacity(0.5);
	}	
}

.popup-layer{
	position: fixed;
  width: 880px;
	height: 596px;
	top: calc(0px - 596px);
	left: calc(50% - 440px);
	z-index: 100;
	@include animate(transition, all, .4s, ease-out);
	
	&.in{
		top: calc(50% - 298px);
	}
	
	.popup-head{
		position: relative;
    height: 60px;    
    width: 100%;
    padding: 14px 20px;
    display: block;
    background-color: #f5f5f5;
    
    title{
    	margin: 0;
	    font-size: 18px;
	    font-weight: 400;
	    line-height: 32px;
	    color: #424242;	
    }
    
    .close{
    	position: absolute;
    	top: 14px;
	    right: 14px;
	    width: 30px;
	    height: 30px;
	    color: #757575;
	    cursor: pointer;
	    transition: all .2s;
	    z-index: 10;
	    border-radius: 15px;
	    line-height: 30px;
	    text-indent: 7px;
	    
	    &:hover{
	    	color: #fff;
    		background-color: #e53935;	
	    }
    }
	}
}

